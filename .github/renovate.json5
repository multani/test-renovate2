{
  "extends": ["github>multani/test-renovate-config:team1"],

  "prHourlyLimit": 50,

  "packageRules": [
    {
      "matchPaths": ["dev/**"],
      "additionalBranchPrefix": "{{baseDir}}/",
      "commitMessageSuffix": " - `{{baseDir}}`",
    },
    {
      "matchPaths": ["int/**"],
      "additionalBranchPrefix": "{{baseDir}}/",
      "commitMessageSuffix": " - `{{baseDir}}`",
    },
    {
      "matchPaths": ["prod/**"],
      "additionalBranchPrefix": "{{baseDir}}/",
      "commitMessageSuffix": " - `{{baseDir}}`",
    },
  ]
}
